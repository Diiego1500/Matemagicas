               <div class="col-10">                            <input class="form-control" type="text" id="username" name="_username"                                   placeholder="Ingresar usuario" value="{{ last_username }}" required="required"/>                        </div>                    </div>                    <div class="form-group row">                        <label class="col-2 col-form-label" for="password">                            <small><img src="{{ asset('Img/Pass.svg') }}"/></small>                        </label>                        <div class="col-10">                            <input class="form-control" type="password" id="password" name="_password"                                   placeholder="Ingresar contraseña" required="required"/>                        </div>                    </div>                    <div class="form-group">                        <div class="col-sm-3"></div>                    </div>                    <input type="checkbox" id="remember_me" name="_remember_me" checked />                    <label for="remember_me">Mantener sesión iniciada</label>                    <div class="form-group row text-center">                        <div class="col-sm-3"></div>                        <div class="col-sm-6">                            <input class="btn btn-lg btn-outline-success" type="submit" id="_submit" name="_submit"value="Entrar"/>                        </div>                        <div class="col-sm-3"></div>                    </div>                </form>            </div>        </div>    </div>    <div class="col-md-12 text-center">        <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModalCenter">            ¿Aún no tienes una cuenta?        </button>        <hr>        <a href="#"  data-toggle="modal" data-target="#ForgotPassword" ><strong>¿Olvidaste tu contraseña?</strong></a>    </div></div><div class="modal fade" id="ForgotPassword" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle2" aria-hidden="true">    <div class="modal-dialog modal-dialog-centered" role="document">        <div class="modal-content">            <div class="modal-header">                <h5 class="modal-title" id="exampleModalLongTitle">Recupera tu cuenta</h5>                <button type="button" class="close" data-dismiss="modal" aria-label="Close">                    <span aria-hidden="true">&times;</span>                </button>            </div>            <div class="modal-body">                <small>Correo con el que te registraste</small>                <input id="forgotuserEmail" class="form-control" type="email" placeholder="Usuario@ejemplo.com">                <button id="BtnForgot" class="btn btn-success mt-3">Confirmar</button>            </div>        </div>    </div></div><!-- Modal --><div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle"     aria-hidden="true">    <div class="modal-dialog modal-dialog-centered modal-lg" role="document">        <div class="modal-content">            <div class="modal-header">                <h5 class="modal-title" id="exampleModalLongTitle">Formulario de registro</h5>                <button type="button" class="close" data-dismiss="modal" aria-label="Close">                    <span aria-hidden="true">&times;</span>                </button>            </div>            <div class="modal-body">                <div class="container text-center">                    {{ form_start(registerForm) }}                    <div class="form-group row">                        <label for="formGroupExampleInput" class="col-sm-3 col-form-label">Email: </label>                        <div class="col-sm-9">                            {{ form_widget(registerForm.email , {'attr':{'class':'form-control'}} ) }}                        </div>                    </div>                    <div class="form-group row">                        <label for="formGroupExampleInput" class="col-sm-3 col-form-label">Usuario: </label>                        <div class="col-sm-9">                            {{ form_widget(registerForm.username , {'attr':{'class':'form-control'}} ) }}                        </div>                    </div>                    <div class="form-group row">                        <label for="formGroupExampleInput" class="col-sm-3 col-form-label">Contraseña: </label>                        <div class="col-sm-9">                            {{ form_widget(registerForm.password , {'attr':{'class':'form-control'}} ) }}                        </div>                    </div>                    {{ form_end(registerForm) }}                    <button id="Register" class="btn btn-success">Registrar</button>                </div>            </div>            <div class="modal-footer">            </div>        </div>    </div></div>